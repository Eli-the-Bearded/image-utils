__END32


}

tmp=/tmp/color$$
in=
out=
size=

inv=no
keep=no

while [ "X$1" != X ] ; do
  arg="$1"; shift

  case "X$arg" in
    X-d|X--dir*) tmp=$1;  shift ;;
    X-i|X--inv*) inv=yes        ;;
    X-k|X--kee*) keep=yes       ;;    
    X-s|X--siz*) size=$1; shift ;;

    X--help) usage ;;

    X-*) echo "$0: use --help for usage, unknown option $arg" ;;

    X*) if [ "X$in" = X ] ; then
          in="$arg"
	else
          out="$arg"
	fi
	;;
  esac

done

if [ "X$in" = X ] ; then 
  echo "$0: need infile"
  exit
fi
if [ "X$out" = X ] ; then 
  echo "$0: need outfile"
  exit
fi

if [ $inv = yes ] ; then 
  invtool=pnminvert
  redcolor='#0ff' # based on inverting colors in original
  grncolor='#f0f' # based on inverting colors in original
  blucolor='#ff0' # based on inverting colors in original
else
  invtool=cat
  redcolor='#f00'       # non-invert
  grncolor='#0f0'       # non-invert
  blucolor='#00f'       # non-invert
fi

if [ $keep = yes ] ; then 
  echo "$0: putting files in $tmp";
fi

mkdir -p "$tmp"
mkfonts "$tmp"


anytopnm "$in" | $invtool | (cd "$tmp" && ppmtorgb3 )

testfile="$tmp/noname.red"
if [ ! -f "$testfile" ] ; then
  echo "Did not seem to work, no $testfile"
  exit
fi


if [ "X$size" != X ] ; then 
  xsize=$( pnminfo --xonly < "$testfile" )
  ysize=$( pnminfo --yonly < "$testfile" )

  if expr $xsize \> $ysize >/dev/null ; then 
    edge=width
  else
    edge=height
  fi

  scale="pnmscale -$edge $size"
else
  scale=cat
fi

frames=
maxx=1
maxy=1

#or cname in red grn blu ; do
# pgm="$tmp/noname.$cname"

for font in 10 12 14 18 24 32 ; do 

  # ps: prescale down
  # 1/5 for smallest, proportional (by font image width) for rest
  #
  # fs: font scale correction for h/w disparity
  # Put largish (> 500 pixel) square image in, measure dimensions of
  # pbmtext output, then
  # $ bc
  # scale=5
  # width / height

  case $font in 
    10) fc=.60102 	# font10.pbm
	ps=.2
	cname=red
	;;
    12) fc=.53938 	# font12.pbm
        ps=.17142	# 96 / 112 / 5 
	cname=grn
	;;
    14) fc=.53424 	# font14.pbm
        ps=.15		# 96 / 128 / 5
	cname=blu
	;;
    18) fc=.57994 	# font18.pbm
        ps=.10909	# 96 / 176 / 5
	cname=red
	;;
    24) fc=.53971 	# font24.pbm
        ps=.08533	# 96 / 225 / 5
	cname=grn
	;;
    32) fc=.55978 	# font32.pbm
        ps=.06315	# 96 / 304 / 5
	cname=blu
	;;
  esac

  pgm="$tmp/noname.$cname"
  frm="$tmp/tmp-$cname"

  case $cname in
     red) color="$redcolor" ;;
     grn) color="$grncolor" ;;
     blu) color="$blucolor" ;;
  esac

  < $pgm pnmscale $ps | pnmscale -yscale $fc | \
    pgmnorm 2>/dev/null | pgmtoascii | \
    tee "$tmp/text-$cname" | pbmtext -font "$tmp/font$font.pbm" | \
    pnmdepth 8 2>/dev/null | $scale 2>/dev/null | pgmnorm 2>/dev/null \
    | pnmcrop -white \
    > "$frm"

  xsize=$( pnminfo --xonly < "$frm" )
  ysize=$( pnminfo --yonly < "$frm" )
  ppmmake "$color" $xsize $ysize > "$frm-block"

  if expr $xsize \> $maxx > /dev/null ; then
    maxx=$xsize
  fi
  if expr $ysize \> $maxy > /dev/null ; then
    maxy=$ysize
  fi

  pamarith -max "$frm-block" "$frm" \
   > "$tmp/f$font-$cname.ppm"


  frames="$frames $tmp/f$font-$cname.ppm"
# done
done

echo "Max sizes: $maxx x $maxy"

last=
for frame in $frames ; do
  xsize=$( pnminfo --xonly < "$frame" )
  ysize=$( pnminfo --yonly < "$frame" )

  xdif=$(( $maxx - $xsize ))
  ydif=$(( $maxy - $ysize ))

  top=$(( $ydif / 2 ))
  bot=$(( $ydif - $top ))
  lef=$(( $xdif / 2 ))
  rig=$(( $xdif - $lef ))

  < "$frame" \
    pnmpad -white -left $lef -right $rig -top $top -bottom $bot \
    > "$frame.pad"

  if [ "X$last" != X ] ; then
    pamarith -min "$last" "$frame.pad" > "$tmp/working.ppm"
    cp "$tmp/working.ppm" "$tmp/staging.ppm"
    last="$tmp/staging.ppm"
  else
    last="$frame.pad"
  fi
done

cp "$tmp/working.ppm" "$out"

if [ $keep != yes ] ; then 
  rm -r "$tmp"
fi
