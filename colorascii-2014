# :r! perl -we 'for $s (10,12,14,18,24,32){print qq,mmencode -u > "\$dir/font$s.pbm" <<\x27__END$s\x27\n,; print qx( mmencode ggfixed-$s.pbm ); print "__END$s\n\n"} '
