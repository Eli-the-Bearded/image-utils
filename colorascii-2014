#!/bin/ksh
# Runs pgmtoascii six times at different scales, uses pbmtext
# with six differently sized fonts (included herein), colorizes
# the output, 2 images per color, and then assembles into a
# final image.
# Aug 2014

usage() {
  echo "$0: usage:"
  echo "  colorascii-2014 [ OPTIONS ] INPUTFILE OUTPUT.PPM"
  echo "Options:"
  echo "  -d TMPDIR  --directory TMPDIR   use TMPDIR for temp files"
  echo "  -i         --invert             run pnminvert over input"
  echo "  -k         --keep               keep temp files"
  echo "  -s SIZE    --size SIZE          target size (largest dimension)"
  exit
}

mkfonts () {
  dir="$1"


cat > "$dir"/font-source.html <<'__ENDSRC'
<html>
<head>
<title>Fixed Fonts; made Aug 2014; used with Firefox 31</title>
<style type=text/css><!--
Body     { background: white; color: black; }
.p10      { font-size: 10px; }
.p12      { font-size: 12px; }
.p14      { font-size: 14px; }
.p18      { font-size: 18px; }
.p24      { font-size: 24px; }
.p32      { font-size: 32px; }
--></style></head>
<body>
<p>10px</p>
<pre class=p10>
                  M ",/^_[`jpqy| M

                  /  !"#$%&'()*+ /
                  < ,-./01234567 <
                  > 89:;<=>?@ABC >
                  @ DEFGHIJKLMNO @
